<!DOCTYPE html>
<html lang="en">

<head>
    <% include ./partials/_header %>
</head>

<body>
    <ul>
        <% for(var url in urls) { %>
            <li>
                <%=url%>:
                    <%= urls[url] %>
                        <form method="POST" action="/urls/<%= url %>/delete">
                        <a href="/urls/<%= url %>">Edit</a>
                        <button type=submit>Delete</button>
                        </form>
            </li>
            <% } %>
    </ul>
</body>

</html>
